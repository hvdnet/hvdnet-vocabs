@prefix dct: <http://purl.org/dc/terms/> .
@prefix qsv: <https://w3id.org/hvdnet/qsv/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix stats: <https://w3id.org/hvdnet/statistics/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

qsv: a skos:ConceptScheme ;
    dct:title "QSV Vocabulary [DRAFT]"@en ;
    dct:description "This vocabulary provides a controlled set of concepts for describing statistical measures and summary statistics generated by the QSV Data Wrangling package."@en ;
    dct:source <https://qsv.dathere.com/> ;
    dct:created "2025-12-22"^^xsd:date ;
    skos:hasTopConcept qsv:statsCmd, qsv:moarstatsCmd ;
    dct:license <https://creativecommons.org/publicdomain/zero/1.0/> .
qsv:statsCmd a skos:Concept ;
    skos:prefLabel "stats command"@en ;
    skos:inScheme qsv: ;
    skos:definition "Statistical measures and summary statistics generated by the 'stats' command in the QSV Data Wrangling package."@en ;
    rdfs:seeAlso <https://github.com/dathere/qsv/blob/main/src/cmd/stats.rs> ;
    skos:narrower
        qsv:antimode, qsv:antimode_count, qsv:antimode_occurrences, qsv:avg_length, qsv:cardinality, qsv:cv, qsv:cv_length,
        qsv:geometric_mean, qsv:harmonic_mean, qsv:iqr, qsv:is_ascii, qsv:lower_inner_fence, qsv:lower_outer_fence, qsv:mad,
        qsv:max, qsv:max_length, qsv:max_precision, qsv:mean, qsv:min, qsv:min_length, qsv:mode, qsv:mode_count, qsv:mode_occurrences,
        qsv:n_negative, qsv:n_positive, qsv:n_zero, qsv:nullcount, qsv:percentiles, qsv:q1, qsv:q2_median, qsv:q3, qsv:range,
        qsv:sem, qsv:skewness, qsv:sort_order, qsv:sortiness, qsv:sparsity, qsv:stddev, qsv:stddev_length, qsv:sum, qsv:sum_length,
        qsv:type, qsv:uniqueness_ratio, qsv:upper_inner_fence, qsv:upper_outer_fence, qsv:variance, qsv:variance_length .

qsv:moarstatsCmd a skos:Concept ;
    skos:prefLabel "moarstats command"@en ;
    skos:inScheme qsv: ;
    skos:definition "Additional statistical measures and summary statistics generated by the 'moarstats' command in the QSV Data Wrangling package."@en ;
    rdfs:seeAlso <https://github.com/dathere/qsv/blob/main/src/cmd/moarstats.rs> ;
    skos:narrower
        qsv:gini_coefficient, qsv:iqr_range_ratio, qsv:kurtosis, qsv:lower_outer_fence_zscore, qsv:mad_stddev_ratio, qsv:max_zscore, qsv:median_mean_ratio,
        qsv:min_zscore, qsv:mode_zscore, qsv:non_outliers_cv, qsv:non_outliers_mean, qsv:non_outliers_stddev, qsv:non_outliers_variance,
        qsv:outlier_impact, qsv:outlier_impact_ratio, qsv:outliers_cv, qsv:outliers_extreme_lower_cnt, qsv:outliers_extreme_upper_cnt,
        qsv:outliers_max, qsv:outliers_mean, qsv:outliers_mild_lower_cnt, qsv:outliers_mild_upper_cnt, qsv:outliers_min, qsv:outliers_normal_cnt,
        qsv:outliers_normal_stddev_ratio, qsv:outliers_percentage, qsv:outliers_range, qsv:outliers_stddev, qsv:outliers_to_normal_mean_ratio,
        qsv:outliers_total_cnt, qsv:outliers_variance, qsv:pearson_skewness, qsv:quartile_coefficient_dispersion, qsv:range_stddev_ratio,
        qsv:relative_standard_error, qsv:trimmed_25pct_stddev_ratio, qsv:trimmed_cv_25pct, qsv:trimmed_mean_25pct, qsv:trimmed_range_25pct,
        qsv:trimmed_stddev_25pct, qsv:trimmed_variance_25pct, qsv:upper_outer_fence_zscore, qsv:winsorized_25pct_stddev_ratio, qsv:winsorized_cv_25pct,
        qsv:winsorized_mean_25pct, qsv:winsorized_range_25pct, qsv:winsorized_stddev_25pct, qsv:winsorized_variance_25pct .

# Unmapped (no skos:exactMatch yet):
# stats: qsv:type, qsv:sort_order, qsv:sortiness, qsv:sum_length, qsv:variance_length, qsv:cv_length, qsv:max_precision,
# qsv:sparsity, qsv:lower_outer_fence, qsv:lower_inner_fence, qsv:upper_inner_fence, qsv:upper_outer_fence, qsv:skewness,
# qsv:mode, qsv:mode_count, qsv:mode_occurrences, qsv:antimode, qsv:antimode_count, qsv:antimode_occurrences, qsv:percentiles.
# moarstats: qsv:relative_standard_error, qsv:min_zscore, qsv:max_zscore, qsv:median_mean_ratio, qsv:iqr_range_ratio, qsv:mad_stddev_ratio,
# qsv:kurtosis, qsv:outliers_extreme_lower_cnt, qsv:outliers_mild_lower_cnt, qsv:outliers_normal_cnt, qsv:outliers_mild_upper_cnt,
# qsv:outliers_extreme_upper_cnt, qsv:outliers_total_cnt, qsv:outliers_mean, qsv:non_outliers_mean, qsv:outliers_to_normal_mean_ratio,
# qsv:outliers_min, qsv:outliers_max, qsv:outliers_range, qsv:outliers_stddev, qsv:outliers_variance, qsv:non_outliers_stddev,
# qsv:non_outliers_variance, qsv:outliers_cv, qsv:non_outliers_cv, qsv:outliers_percentage, qsv:outlier_impact, qsv:outlier_impact_ratio,
# qsv:outliers_normal_stddev_ratio, qsv:lower_outer_fence_zscore, qsv:upper_outer_fence_zscore, qsv:trimmed_stddev_25pct,
# qsv:trimmed_variance_25pct, qsv:winsorized_stddev_25pct, qsv:winsorized_variance_25pct, qsv:trimmed_cv_25pct, qsv:winsorized_cv_25pct,
# qsv:trimmed_25pct_stddev_ratio, qsv:winsorized_25pct_stddev_ratio, qsv:trimmed_range_25pct, qsv:winsorized_range_25pct.
# Minimal concept stubs (can be enriched later)

qsv:type a skos:Concept ;
    skos:prefLabel "type"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The inferred QSV field type for a column (e.g., string, integer, float, date)."@en .

qsv:is_ascii a skos:Concept ;
    skos:prefLabel "is_ascii"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:IsAscii ;
    skos:definition "Whether all non-null values contain only ASCII characters."@en .

qsv:sum a skos:Concept ;
    skos:prefLabel "sum"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:Sum ;
    skos:definition "The sum of all numeric values."@en .

qsv:min a skos:Concept ;
    skos:prefLabel "min"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:Minimum ;
    skos:definition "The minimum observed value."@en .

qsv:max a skos:Concept ;
    skos:prefLabel "max"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:Maximum ;
    skos:definition "The maximum observed value."@en .

qsv:range a skos:Concept ;
    skos:prefLabel "range"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:Range ;
    skos:definition "The difference between the maximum and minimum values (max − min)."@en .

qsv:sort_order a skos:Concept ;
    skos:prefLabel "sort_order"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "A categorical indicator of whether values appear sorted (e.g., ascending, descending, or unsorted)."@en .

qsv:sortiness a skos:Concept ;
    skos:prefLabel "sortiness"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "A numeric score indicating how close the column is to being sorted (higher means more sorted)."@en .

qsv:min_length a skos:Concept ;
    skos:prefLabel "min_length"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:MinLength ;
    skos:definition "The minimum string length among non-null values."@en .

qsv:max_length a skos:Concept ;
    skos:prefLabel "max_length"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:MaxLength ;
    skos:definition "The maximum string length among non-null values."@en .

qsv:sum_length a skos:Concept ;
    skos:prefLabel "sum_length"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The total string length across all non-null values (sum of character counts)."@en .

qsv:avg_length a skos:Concept ;
    skos:prefLabel "avg_length"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:AverageLength ;
    skos:definition "The average string length of non-null values."@en .

qsv:stddev_length a skos:Concept ;
    skos:prefLabel "stddev_length"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:LengthStdDev ;
    skos:definition "The standard deviation of string lengths across non-null values."@en .

qsv:variance_length a skos:Concept ;
    skos:prefLabel "variance_length"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The variance of string lengths across non-null values."@en .

qsv:cv_length a skos:Concept ;
    skos:prefLabel "cv_length"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The coefficient of variation of string lengths (stddev_length divided by avg_length)."@en .

qsv:mean a skos:Concept ;
    skos:prefLabel "mean"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:ArithmeticMean ;
    skos:definition "The arithmetic mean (average) of numeric values."@en .

qsv:sem a skos:Concept ;
    skos:prefLabel "sem"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:StandardErrorOfMean ;
    skos:definition "The standard error of the mean (estimated standard deviation of the sample mean)."@en .

qsv:geometric_mean a skos:Concept ;
    skos:prefLabel "geometric_mean"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:GeometricMean ;
    skos:definition "The geometric mean of numeric values."@en .

qsv:harmonic_mean a skos:Concept ;
    skos:prefLabel "harmonic_mean"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:HarmonicMean ;
    skos:definition "The harmonic mean of numeric values."@en .

qsv:stddev a skos:Concept ;
    skos:prefLabel "stddev"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:StandardDeviation ;
    skos:definition "The standard deviation of numeric values."@en .

qsv:variance a skos:Concept ;
    skos:prefLabel "variance"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:Variance ;
    skos:definition "The variance of numeric values."@en .

qsv:cv a skos:Concept ;
    skos:prefLabel "cv"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:CoefficientOfVariation ;
    skos:definition "The coefficient of variation of numeric values (stddev divided by mean)."@en .

qsv:nullcount a skos:Concept ;
    skos:prefLabel "nullcount"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:NullCases ;
    skos:definition "The number of null or missing values."@en .

qsv:n_negative a skos:Concept ;
    skos:prefLabel "n_negative"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:NegativeCount ;
    skos:definition "The number of numeric values that are negative."@en .

qsv:n_zero a skos:Concept ;
    skos:prefLabel "n_zero"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:ZeroCount ;
    skos:definition "The number of numeric values that are exactly zero."@en .

qsv:n_positive a skos:Concept ;
    skos:prefLabel "n_positive"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:PositiveCount ;
    skos:definition "The number of numeric values that are positive."@en .

qsv:max_precision a skos:Concept ;
    skos:prefLabel "max_precision"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The maximum number of decimal places observed for numeric values."@en .

qsv:sparsity a skos:Concept ;
    skos:prefLabel "sparsity"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "A measure of missingness, indicating how sparse the column is due to null or empty values."@en .

qsv:mad a skos:Concept ;
    skos:prefLabel "mad"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:MedianAbsoluteDeviation ;
    skos:definition "The median absolute deviation (median of |x − median(x)|)."@en .

qsv:lower_outer_fence a skos:Concept ;
    skos:prefLabel "lower_outer_fence"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The lower outer Tukey fence for outliers (typically Q1 − 3×IQR)."@en .

qsv:lower_inner_fence a skos:Concept ;
    skos:prefLabel "lower_inner_fence"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The lower inner Tukey fence for outliers (typically Q1 − 1.5×IQR)."@en .
qsv:q1 a skos:Concept ;
    skos:prefLabel "q1"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:FirstQuartile ;
    skos:definition "The first quartile (25th percentile)."@en .

qsv:q2_median a skos:Concept ;
    skos:prefLabel "q2_median"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:Median ;
    skos:definition "The median (50th percentile)."@en .

qsv:q3 a skos:Concept ;
    skos:prefLabel "q3"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:ThirdQuartile ;
    skos:definition "The third quartile (75th percentile)."@en .

qsv:iqr a skos:Concept ;
    skos:prefLabel "iqr"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:InterquartileRange ;
    skos:definition "The interquartile range (Q3 − Q1)."@en .
qsv:upper_inner_fence a skos:Concept ;
    skos:prefLabel "upper_inner_fence"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The upper inner Tukey fence for outliers (typically Q3 + 1.5×IQR)."@en .

qsv:upper_outer_fence a skos:Concept ;
    skos:prefLabel "upper_outer_fence"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The upper outer Tukey fence for outliers (typically Q3 + 3×IQR)."@en .

qsv:skewness a skos:Concept ;
    skos:prefLabel "skewness"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "A measure of asymmetry of the numeric value distribution."@en .
qsv:cardinality a skos:Concept ;
    skos:prefLabel "cardinality"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:UniqueCases ;
    skos:definition "The number of distinct (unique) non-null values."@en .

qsv:uniqueness_ratio a skos:Concept ;
    skos:prefLabel "uniqueness_ratio"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:exactMatch stats:DistinctnessRatio ;
    skos:definition "The fraction of distinct values among non-null values (cardinality divided by valid count)."@en .

qsv:mode a skos:Concept ;
    skos:prefLabel "mode"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The most frequently occurring non-null value."@en .

qsv:mode_count a skos:Concept ;
    skos:prefLabel "mode_count"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The frequency (count) of the mode value."@en .

qsv:mode_occurrences a skos:Concept ;
    skos:prefLabel "mode_occurrences"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The number of distinct values tied for the highest frequency (the number of modes)."@en .

qsv:antimode a skos:Concept ;
    skos:prefLabel "antimode"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "A least-frequent value among those that occur at least once (the rarest observed value)."@en .

qsv:antimode_count a skos:Concept ;
    skos:prefLabel "antimode_count"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The minimum non-zero frequency among observed values (count of the antimode)."@en .

qsv:antimode_occurrences a skos:Concept ;
    skos:prefLabel "antimode_occurrences"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "The number of distinct values tied for the minimum non-zero frequency (the number of antimodes)."@en .

qsv:percentiles a skos:Concept ;
    skos:prefLabel "percentiles"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:statsCmd ;
    skos:definition "One or more percentile values (e.g., p10, p25, p50) computed for numeric columns."@en .

# moarstats concept stubs

qsv:pearson_skewness a skos:Concept ;
    skos:prefLabel "pearson_skewness"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:exactMatch stats:PearsonSkewness ;
    skos:definition "Pearson’s skewness coefficient (a skewness estimate based on mean, median, and standard deviation)."@en .

qsv:range_stddev_ratio a skos:Concept ;
    skos:prefLabel "range_stddev_ratio"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:exactMatch stats:StdDevRangeRatio ;
    skos:definition "The ratio of the range to the standard deviation (range divided by stddev)."@en .

qsv:quartile_coefficient_dispersion a skos:Concept ;
    skos:prefLabel "quartile_coefficient_dispersion"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:exactMatch stats:QuartileDispersionCoefficient ;
    skos:definition "The quartile dispersion coefficient, typically (Q3 − Q1) / (Q3 + Q1)."@en .

qsv:mode_zscore a skos:Concept ;
    skos:prefLabel "mode_zscore"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:exactMatch stats:ModeZScore ;
    skos:definition "The z-score of the mode value (mode minus mean, divided by stddev)."@en .

qsv:relative_standard_error a skos:Concept ;
    skos:prefLabel "relative_standard_error"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "The standard error of the mean divided by the mean (SEM/mean)."@en .

qsv:min_zscore a skos:Concept ;
    skos:prefLabel "min_zscore"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "The z-score of the minimum value: (min − mean) / stddev."@en .

qsv:max_zscore a skos:Concept ;
    skos:prefLabel "max_zscore"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "The z-score of the maximum value: (max − mean) / stddev."@en .

qsv:median_mean_ratio a skos:Concept ;
    skos:prefLabel "median_mean_ratio"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "The ratio of the median to the mean (median/mean)."@en .

qsv:iqr_range_ratio a skos:Concept ;
    skos:prefLabel "iqr_range_ratio"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "The ratio of interquartile range to range (IQR/range)."@en .

qsv:mad_stddev_ratio a skos:Concept ;
    skos:prefLabel "mad_stddev_ratio"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "The ratio of median absolute deviation to standard deviation (MAD/stddev)."@en .

qsv:kurtosis a skos:Concept ;
    skos:prefLabel "kurtosis"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "A measure of tail-heaviness/peakedness of the distribution (kurtosis)."@en .

qsv:gini_coefficient a skos:Concept ;
    skos:prefLabel "gini_coefficient"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:exactMatch stats:GiniCoefficient ;
    skos:definition "The Gini coefficient measuring inequality/dispersion of numeric values (0 = equal, 1 = maximal inequality)."@en .
qsv:outliers_extreme_lower_cnt a skos:Concept ;
    skos:prefLabel "outliers_extreme_lower_cnt"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "Count of extreme lower outliers (values below the lower outer fence, typically Q1 − 3×IQR)."@en .

qsv:outliers_mild_lower_cnt a skos:Concept ;
    skos:prefLabel "outliers_mild_lower_cnt"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "Count of mild lower outliers (between lower outer and lower inner fences)."@en .

qsv:outliers_normal_cnt a skos:Concept ;
    skos:prefLabel "outliers_normal_cnt"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "Count of values within the inner fences (non-outliers by the IQR rule)."@en .

qsv:outliers_mild_upper_cnt a skos:Concept ;
    skos:prefLabel "outliers_mild_upper_cnt"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "Count of mild upper outliers (between upper inner and upper outer fences)."@en .

qsv:outliers_extreme_upper_cnt a skos:Concept ;
    skos:prefLabel "outliers_extreme_upper_cnt"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "Count of extreme upper outliers (values above the upper outer fence, typically Q3 + 3×IQR)."@en .

qsv:outliers_total_cnt a skos:Concept ;
    skos:prefLabel "outliers_total_cnt"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "Total count of outliers outside the inner fences (mild + extreme)."@en .

qsv:outliers_mean a skos:Concept ;
    skos:prefLabel "outliers_mean"@en ;
    skos:inScheme qsv: ;
    skos:broader qsv:moarstatsCmd ;
    skos:definition "The mean of values classified as outliers."@en .

qsv:non_outliers_mean a skos:Concept ;
