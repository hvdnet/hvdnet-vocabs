    skos:scopeNote "Compute the fraction of invalid observations and multiply by 100 to express as a percentage."@en ;
    stats:formula "$100 \\times \\frac{n_{invalid}}{n}$" ;
    stats:computationMethod "Count invalid values; divide by total n; multiply by 100."@en ;
    skos:broader stats:DerivedRatios ;
    skos:inScheme stats: ;
    skos:exactMatch ddi:e592a55 .

stats:QuartileDispersionCoefficient a skos:Concept ;
    skos:prefLabel "Quartile Coefficient of Dispersion"@en ;
    skos:definition "A relative measure of dispersion based on quartiles: (Q3 - Q1) / (Q3 + Q1)."@en ;
    skos:scopeNote "Estimate Q1 and Q3; take their difference over their sum."@en ;
    stats:formula "$\\frac{Q_3 - Q_1}{Q_3 + Q_1}$" ;
    stats:computationMethod "Compute Q1 and Q3 under a chosen convention; apply formula."@en ;
    skos:broader stats:DerivedRatios ;
    skos:inScheme stats: .

stats:ModeFrequencyRatio a skos:Concept ;
    skos:prefLabel "Mode frequency ratio"@en ;
