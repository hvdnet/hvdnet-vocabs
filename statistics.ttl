    skos:scopeNote "Tokenize to words and sum counts across all values."@en ;
    stats:formula "$\\sum \\text{wc}(x_i)$" ;
    stats:computationMethod "Count words per value and sum across the dataset."@en ;
    skos:broader stats:TextMeasures ;
    skos:inScheme stats: .

stats:UniqueWordCount a skos:Concept ;
    skos:prefLabel "Unique word count"@en ;
    skos:definition "The number of distinct words appearing across all text values."@en ;
    skos:scopeNote "Normalize case and optionally strip punctuation; take set cardinality across all words."@en ;
    stats:computationMethod "Tokenize texts; normalize tokens; compute number of unique tokens across the corpus."@en ;
    skos:broader stats:TextMeasures ;
    skos:inScheme stats: .

stats:LexicalDiversity a skos:Concept ;
    skos:prefLabel "Lexical diversity"@en ;
    skos:definition "The ratio of unique words to total words, measuring vocabulary richness."@en ;
    skos:scopeNote "Compute type-token ratio (unique/total) or alternative normalized measures as needed."@en ;
    stats:formula "$\\frac{\\text{unique words}}{\\text{total words}}$" ;
    stats:computationMethod "Tokenize and normalize; count unique and total tokens; take ratio."@en ;
    skos:broader stats:TextMeasures ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q18346297 .

stats:Languages a skos:Concept ;
    skos:prefLabel "Languages"@en ;
    skos:definition "The set of natural languages detected or present in text values."@en ;
    skos:scopeNote "Apply a language detection algorithm per text; aggregate detected language set or distribution."@en ;
    stats:computationMethod "Run language ID per value; collect unique languages and optionally their frequencies."@en ;
    skos:broader stats:TextMeasures ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q34770 .

# Concepts - Metadata

stats:IsAscii a skos:Concept ;
    skos:prefLabel "ASCII characters only"@en ;
    skos:definition "Indicates whether values contain only ASCII characters."@en ;
    skos:broader stats:Metadata ;
    skos:inScheme stats: .

# Concepts - Missingness/Completeness

stats:MissingRate a skos:Concept ;
    skos:prefLabel "Missing rate"@en ;
    skos:altLabel "Null rate"@en ;
    skos:definition "The proportion of values that are missing or null."@en ;
    skos:scopeNote "Divide the number of missing/null entries by the total number of observations."@en ;
    stats:formula "$\\text{MissingRate} = \\frac{m}{n}$" ;
    stats:computationMethod "Count missing/null values (m) and divide by total observations (n)."@en ;
    skos:broader stats:MissingnessCompleteness ;
    skos:inScheme stats: .

stats:NonNullRate a skos:Concept ;
    skos:prefLabel "Non-null rate"@en ;
    skos:altLabel "Completeness"@en ;
    skos:definition "The proportion of values that are present and non-null."@en ;
    skos:scopeNote "Divide the number of present, non-null entries by the total number of observations."@en ;
    stats:formula "$\\text{NonNullRate} = \\frac{n_{valid}}{n}$" ;
    stats:computationMethod "Count non-null, valid values and divide by total observations (n)."@en ;
    skos:broader stats:MissingnessCompleteness ;
    skos:inScheme stats: .

stats:DuplicateRows a skos:Concept ;
    skos:prefLabel "Duplicate rows"@en ;
    skos:altLabel "Duplicate count"@en ;
    skos:definition "The number of duplicated records based on chosen key(s)."@en ;
    skos:broader stats:MissingnessCompleteness ;
    skos:inScheme stats: .

stats:DistinctnessRatio a skos:Concept ;
    skos:prefLabel "Distinctness ratio"@en ;
    skos:altLabel "Uniqueness ratio"@en ;
    skos:definition "The fraction of distinct values relative to total observations."@en ;
    skos:broader stats:MissingnessCompleteness ;
    skos:inScheme stats: .

# Concepts - File-level Statistics/Attributes

stats:FileRecordCount a skos:Concept ;
    skos:prefLabel "Total record count"@en ;
    skos:altLabel "Row count"@en ;
    skos:definition "The total number of records (rows) in the file."@en ;
    skos:scopeNote "Count the number of data records (rows) in the file, excluding header rows if present."@en ;
    stats:formula "$n_{\\text{rows}}$" ;
    stats:computationMethod "Parse the file structure and count all data rows, excluding header or metadata rows."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileVariableCount a skos:Concept ;
    skos:prefLabel "Total variable/column count"@en ;
    skos:altLabel "Column count"@en ;
    skos:definition "The total number of variables or columns in the file."@en ;
    skos:scopeNote "Count the number of variables (columns) in the file based on the schema or header row."@en ;
    stats:formula "$n_{\\text{cols}}$" ;
    stats:computationMethod "Determine the number of columns from the file header or schema definition."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileSizeBytes a skos:Concept ;
    skos:prefLabel "File size (bytes)"@en ;
    skos:definition "The size of the file measured in bytes."@en ;
    skos:scopeNote "Retrieve the file size in bytes from the file system metadata."@en ;
    stats:computationMethod "Query the file system for the byte size of the file."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileChecksum a skos:Concept ;
    skos:prefLabel "File checksum"@en ;
    skos:definition "A hash value used to verify file integrity."@en ;
    skos:scopeNote "Compute a cryptographic hash (e.g., MD5, SHA-256) of the file contents to verify integrity."@en ;
    stats:computationMethod "Read the entire file byte stream and compute the hash using a chosen algorithm (e.g., SHA-256)."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileEncoding a skos:Concept ;
    skos:prefLabel "File character encoding"@en ;
    skos:definition "The character encoding used to represent text in the file (e.g., UTF-8)."@en ;
    skos:scopeNote "Detect or retrieve the character encoding (e.g., UTF-8, ISO-8859-1) from file metadata or content analysis."@en ;
    stats:computationMethod "Inspect file metadata or use encoding detection algorithms to identify the character set."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileCompression a skos:Concept ;
    skos:prefLabel "File compression"@en ;
    skos:definition "The compression format or status applied to the file (e.g., gzip)."@en ;
    skos:scopeNote "Identify the compression format (e.g., gzip, zip, bzip2) or confirm if the file is uncompressed."@en ;
    stats:computationMethod "Check file extension, magic bytes, or metadata to determine compression type."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileSchemaVersion a skos:Concept ;
    skos:prefLabel "File schema/version"@en ;
    skos:definition "The schema identifier or version describing the file's structure."@en ;
    skos:scopeNote "Extract the schema version or identifier from file metadata or embedded schema declarations."@en ;
    stats:computationMethod "Parse metadata fields or embedded schema annotations to retrieve version information."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileLastModifiedTime a skos:Concept ;
    skos:prefLabel "File last modified time"@en ;
    skos:altLabel "Last modified timestamp"@en ;
    skos:definition "The timestamp when the file was last modified."@en ;
    skos:scopeNote "Retrieve the last modified timestamp from file system metadata."@en ;
    stats:computationMethod "Query the file system for the last modified timestamp (e.g., via stat or file metadata APIs)."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileCreationTime a skos:Concept ;
    skos:prefLabel "File creation time"@en ;
    skos:altLabel "Created timestamp"@en ;
    skos:definition "The timestamp when the file was originally created."@en ;
    skos:scopeNote "Retrieve the creation timestamp from file system metadata (may not be available on all systems)."@en ;
    stats:computationMethod "Query the file system for the creation timestamp (e.g., via stat or file metadata APIs)."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:FileMimeType a skos:Concept ;
    skos:prefLabel "File MIME/media type"@en ;
    skos:altLabel "Media type"@en ;
    skos:definition "The media type that describes the file's format (e.g., text/csv)."@en ;
    skos:scopeNote "Determine the MIME type from file extension, magic bytes, or content inspection."@en ;
    stats:computationMethod "Use MIME type detection libraries or inspect file signatures to identify the media type."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

stats:RecordVariableRatio a skos:Concept ;
    skos:prefLabel "Record-to-variable ratio"@en ;
    skos:altLabel "Rows-to-columns ratio"@en ;
    skos:definition "The ratio of records to variables, indicating dataset shape."@en ;
    skos:scopeNote "Compute by dividing the number of records (rows) by the number of variables (columns)."@en ;
    stats:formula "$\\frac{n_{\\text{rows}}}{n_{\\text{cols}}}$" ;
    stats:computationMethod "Divide the total record count by the total variable count."@en ;
    skos:broader stats:FileLevel ;
    skos:inScheme stats: .

# Concepts - Information Content

stats:ShannonEntropy a skos:Concept ;
    skos:prefLabel "Shannon entropy"@en ;
    skos:altLabel "Information entropy"@en ;
    skos:definition "The expected information content per symbol based on its probability distribution."@en ;
    skos:scopeNote "Estimate category probabilities (e.g., via relative frequencies) and apply the entropy formula; choose log base appropriately (e or 2)."@en ;
    stats:formula "$H = - \\sum_{i} p_i \\log p_i$" ;
    stats:computationMethod "Compute relative frequencies p_i over valid categories; apply the entropy formula using chosen logarithm base."@en ;
    skos:broader stats:InformationContent ;
    skos:inScheme stats: .

stats:Perplexity a skos:Concept ;
    skos:prefLabel "Perplexity"@en ;
    skos:altLabel "Language-model perplexity"@en ;
    skos:definition "An exponentiated form of entropy indicating average branching or uncertainty."@en ;
    skos:broader stats:InformationContent ;
    skos:inScheme stats: .

# Concepts - Outlier Flags

stats:OutlierCountIQR a skos:Concept ;
    skos:prefLabel "Outlier count (IQR rule)"@en ;
    skos:altLabel "Tukey fence outliers"@en ;
    skos:definition "The number of observations beyond k×IQR fences from the quartiles."@en ;
    skos:scopeNote "Compute IQR; define lower/upper fences Q1 − k·IQR and Q3 + k·IQR (k typically 1.5 or 3); count values outside."@en ;
    stats:formula "$\\text{Lower} = Q_1 - k\\,\\mathrm{IQR}, \\quad \\text{Upper} = Q_3 + k\\,\\mathrm{IQR}$" ;
    stats:computationMethod "Estimate Q1 and Q3; pick k (e.g., 1.5); count observations below lower fence or above upper fence."@en ;
    skos:broader stats:Outliers ;
    skos:inScheme stats: .

stats:OutlierCountZScore a skos:Concept ;
    skos:prefLabel "Outlier count (Z-score rule)"@en ;
    skos:altLabel "Z-score outliers"@en ;
    skos:definition "The number of observations with standardized values exceeding a threshold (e.g., |z|>3)."@en ;
    skos:scopeNote "Compute z-scores and count observations where |z| exceeds the chosen threshold."@en ;
    stats:formula "$z_i = \\frac{x_i - \\bar{x}}{s}$" ;
    stats:computationMethod "Compute mean and sample standard deviation; standardize each value; count values with absolute z above threshold."@en ;
    skos:broader stats:Outliers ;
    skos:inScheme stats: .

# Concepts - Categorical Profiling

stats:TopKCategories a skos:Concept ;
    skos:prefLabel "Top-k categories"@en ;
    skos:altLabel "Top categories"@en ;
    skos:definition "The most frequent k categories ranked by count."@en ;
    skos:scopeNote "Count the frequency of each category, sort in descending order, and select the top k most frequent categories."@en ;
    stats:computationMethod "Aggregate value counts by category, sort descending by frequency, and return the first k categories."@en ;
    skos:broader stats:CategoricalProfiling ;
    skos:inScheme stats: .

stats:TopKCategoryShare a skos:Concept ;
    skos:prefLabel "Top-k category share"@en ;
    skos:altLabel "Top-k cumulative share"@en ;
    skos:definition "The cumulative proportion contributed by the top k categories."@en ;
    skos:scopeNote "Compute by summing the frequencies of the top k categories and dividing by the total frequency."@en ;
    stats:formula "$\\frac{\\sum_{i=1}^{k} f_i}{\\sum_{j=1}^{m} f_j}$" ;
    stats:computationMethod "Sum counts of the top k categories and divide by the total count of all categories."@en ;
    skos:broader stats:CategoricalProfiling ;
    skos:inScheme stats: .

stats:CardinalityBucket a skos:Concept ;
    skos:prefLabel "Cardinality bucket"@en ;
    skos:altLabel "Cardinality class"@en ;
    skos:definition "A categorization of the number of distinct values into predefined buckets."@en ;
    skos:scopeNote "Classify the cardinality (number of unique values) into buckets (e.g., low, medium, high) based on thresholds."@en ;
    stats:computationMethod "Count distinct values, then map the count to a predefined cardinality bucket based on threshold rules."@en ;
    skos:broader stats:CategoricalProfiling ;
    skos:inScheme stats: .

stats:ImbalanceRatio a skos:Concept ;
    skos:prefLabel "Imbalance ratio"@en ;
    skos:altLabel "Class imbalance ratio"@en ;
    skos:definition "The ratio between majority and minority class frequencies."@en ;
    skos:scopeNote "Compute by dividing the frequency of the most common category by the frequency of the least common category."@en ;
    stats:formula "$\\frac{f_{\\text{max}}}{f_{\\text{min}}}$" ;
    stats:computationMethod "Find the maximum and minimum category frequencies, then divide max by min."@en ;
    skos:broader stats:CategoricalProfiling ;
    skos:inScheme stats: .
