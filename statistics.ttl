@prefix stats: <https://w3id.org/hvdnet/statistics/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wikidata: <https://wikidata.org/wiki/> .
@prefix ddi: <http://rdf-vocabulary.ddialliance.org/cv/SummaryStatisticType/2.1.2/> .

# Crosswalks: Concepts may include skos:exactMatch links to Wikidata
# using the wikidata: prefix above (https://wikidata.org/wiki/).
# Use skos:closeMatch where equivalence is weaker. Future revisions may
# adopt Wikidata entity IRIs (e.g., http://www.wikidata.org/entity/Q19033)
# for improved machine-resolution.

stats: a skos:ConceptScheme ;
    dct:title "Descriptive Statistics Vocabulary"@en ;
    dct:description "The descriptive statistics vocabulary provides a controlled set of concepts for describing statistical measures and summary statistics for FAIR data."@en ;
    dct:created "2025-12-20"^^xsd:date ;
    skos:hasTopConcept stats:CentralTendency, stats:Dispersion, stats:SizeCount, 
        stats:DerivedRatios, stats:ShapeDistribution, stats:TextMeasures, stats:Metadata, stats:FileLevel, stats:MissingnessCompleteness, stats:RobustStatistics, stats:AssociationDependence, stats:InformationContent, stats:Outliers ;
    dct:license <https://creativecommons.org/publicdomain/zero/1.0/> .

# Categories

stats:CentralTendency a skos:Concept ;
    skos:prefLabel "Central Tendency/Location"@en ;
    skos:topConceptOf stats: ;
    skos:narrower 
        stats:ArithmeticMean, stats:GeometricMean, stats:HarmonicMean,
        stats:TrimmedMean, stats:WinsorizedMean, stats:Mode, stats:Median, 
        stats:Minimum, stats:Maximum, stats:Sum, 
        stats:FirstQuartile, stats:SecondQuartile, stats:ThirdQuartile, 
        stats:FirstQuintile, stats:SecondQuintile, stats:ThirdQuintile, stats:FourthQuintile, 
        stats:FirstDecile, stats:SecondDecile, stats:ThirdDecile, stats:FourthDecile, stats:FifthDecile, 
        stats:SixthDecile, stats:SeventhDecile, stats:EighthDecile, stats:NinthDecile, stats:OtherPercentile .

stats:Dispersion a skos:Concept ;
    skos:prefLabel "Dispersion/Variability"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:StandardErrorOfMean, stats:Range, stats:Variance, 
    stats:StandardDeviation, stats:AverageAbsoluteDeviation, stats:MedianAbsoluteDeviation, 
    stats:InterquartileRange, stats:InterquintileRange, stats:InterdecileRange, stats:GiniCoefficient, 
    stats:QnEstimator, stats:SnEstimator .

stats:SizeCount a skos:Concept ;
    skos:prefLabel "Size/Count"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:ValidCases, stats:InvalidCases, stats:NullCases, stats:UniqueCases, 
    stats:Sparsity, stats:PositiveCount, stats:ZeroCount, stats:NegativeCount, 
    stats:SignificantDigits, stats:Frequency, stats:Other .

stats:DerivedRatios a skos:Concept ;
    skos:prefLabel "Other/Derived Ratios"@en ;
    skos:topConceptOf stats: ;  
    skos:narrower stats:ModeZScore, stats:StdDevRangeRatio, stats:CoefficientOfVariation, 
    stats:PercentageOfValidCases, stats:PercentageOfInvalidCases, stats:QuartileDispersionCoefficient, 
    stats:DistinctnessRatio, stats:ModeFrequencyRatio, stats:NonZeroRate, stats:PositiveShare, stats:NegativeShare .

stats:ShapeDistribution a skos:Concept ;
    skos:prefLabel "Shape/Distribution"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:Beta1, stats:Beta2, stats:ExcessKurtosis, stats:FisherSkewness, stats:PearsonSkewness,
        stats:SampleSkewness, stats:PopulationSkewness, stats:SampleExcessKurtosis, stats:PopulationExcessKurtosis,
        stats:BowleySkewnessCoefficient, stats:ShapiroWilk, stats:AndersonDarling, stats:KolmogorovSmirnov, stats:JarqueBera .

stats:TextMeasures a skos:Concept ;
    skos:prefLabel "Text/String Measures"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:MinLength, stats:MaxLength, stats:AverageLength, stats:LengthStdDev, 
    stats:MinWordCount, stats:MaxWordCount, stats:AverageWordCount, stats:TotalWordCount, 
    stats:UniqueWordCount, stats:LexicalDiversity, stats:Languages .

stats:Metadata a skos:Concept ;
    skos:prefLabel "Metadata"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:IsAscii .

stats:FileLevel a skos:Concept ;
    skos:prefLabel "File-level Statistics/Attributes"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:FileRecordCount, stats:FileVariableCount, stats:FileSizeBytes, stats:FileChecksum, 
        stats:FileEncoding, stats:FileCompression, stats:FileSchemaVersion, stats:FileLastModifiedTime, 
        stats:FileCreationTime, stats:FileMimeType, stats:RecordVariableRatio .

stats:MissingnessCompleteness a skos:Concept ;
    skos:prefLabel "Missingness/Completeness"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:MissingRate, stats:NonNullRate, stats:NullCases, stats:InvalidCases, stats:DuplicateRows, stats:DistinctnessRatio .

stats:RobustStatistics a skos:Concept ;
    skos:prefLabel "Robust Location/Scale"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:Midrange, stats:Trimean, stats:BiweightMidvariance, stats:TrimmedMean, stats:WinsorizedMean, 
        stats:QnEstimator, stats:SnEstimator .

stats:AssociationDependence a skos:Concept ;
    skos:prefLabel "Association/Dependence"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:Covariance, stats:PearsonCorrelation, stats:SpearmanCorrelation, stats:KendallCorrelation, stats:MaxAbsCorrelation .

stats:InformationContent a skos:Concept ;
    skos:prefLabel "Information Content"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:ShannonEntropy, stats:Perplexity .

stats:Outliers a skos:Concept ;
    skos:prefLabel "Outlier Flags"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:OutlierCountIQR, stats:OutlierCountZScore .

stats:CategoricalProfiling a skos:Concept ;
    skos:prefLabel "Categorical Profiling"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:TopKCategories, stats:TopKCategoryShare, stats:CardinalityBucket, stats:ImbalanceRatio .

# Concepts - Central Tendency/Location

stats:ArithmeticMean a skos:Concept ;
    skos:prefLabel "Arithmetic mean"@en ;
    skos:definition "The sum of all values divided by the number of values."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q19033, ddi:7975ed0 .

stats:GeometricMean a skos:Concept ;
    skos:prefLabel "Geometric mean"@en ;
    skos:definition "The nth root of the product of n values."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q185049, ddi:cd8ebb0 .

stats:HarmonicMean a skos:Concept ;
    skos:prefLabel "Harmonic mean"@en ;
    skos:definition "The reciprocal of the arithmetic mean of reciprocals."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q188347, ddi:71996c9 .

stats:TrimmedMean a skos:Concept ;
    skos:prefLabel "Trimmed mean"@en ;
    skos:definition "The mean calculated after removing a specified percentage of extreme values from both ends."@en ;
    skos:broader stats:CentralTendency, stats:RobustStatistics ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q32278, ddi:17ff276 .

stats:WinsorizedMean a skos:Concept ;
    skos:prefLabel "Winsorized Mean"@en ;
    skos:definition "The mean calculated after replacing extreme values with less extreme values at specified percentiles."@en ;
    skos:broader stats:CentralTendency, stats:RobustStatistics ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q2989389 .

stats:Mode a skos:Concept ;
    skos:prefLabel "Mode"@en ;
    skos:definition "The most frequently occurring value in a dataset."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q188224, ddi:650be61 .

stats:Median a skos:Concept ;
    skos:prefLabel "Median"@en ;
    skos:definition "The middle value when data are sorted in order."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q226995, ddi:66851a3 .

stats:Minimum a skos:Concept ;
    skos:prefLabel "Minimum"@en ;
    skos:definition "The smallest value in a dataset."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q10585806, ddi:a1d0ec6 .

stats:Maximum a skos:Concept ;
    skos:prefLabel "Maximum"@en ;
    skos:definition "The largest value in a dataset."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q10578722, ddi:8321e79 .

stats:Sum a skos:Concept ;
    skos:prefLabel "Sum"@en ;
    skos:definition "The total obtained by adding all values together."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q218005, ddi:a0ec870 .

stats:FirstQuartile a skos:Concept ;
    skos:prefLabel "First quartile"@en ;
    skos:definition "The 25th percentile, below which 25% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q112468319, ddi:350c70a .

stats:SecondQuartile a skos:Concept ;
    skos:prefLabel "Second quartile"@en ;
    skos:definition "The 50th percentile (median), below which 50% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q226995, ddi:f02fd7a .

stats:ThirdQuartile a skos:Concept ;
    skos:prefLabel "Third quartile"@en ;
    skos:definition "The 75th percentile, below which 75% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q112468502, ddi:ebe9990 .

stats:FirstQuintile a skos:Concept ;
    skos:prefLabel "First quintile"@en ;
    skos:definition "The 20th percentile, below which 20% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3176606, ddi:ccfc0c3 .

stats:SecondQuintile a skos:Concept ;
    skos:prefLabel "Second quintile"@en ;
    skos:definition "The 40th percentile, below which 40% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3176606, ddi:3ca7a64 .

stats:ThirdQuintile a skos:Concept ;
    skos:prefLabel "Third quintile"@en ;
    skos:definition "The 60th percentile, below which 60% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3176606, ddi:e9f3735 .

stats:FourthQuintile a skos:Concept ;
    skos:prefLabel "Fourth quintile"@en ;
    skos:definition "The 80th percentile, below which 80% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3176606, ddi:c3a6a12 .

stats:FirstDecile a skos:Concept ;
    skos:prefLabel "First decile"@en ;
    skos:definition "The 10th percentile, below which 10% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:ac62c25 .

stats:SecondDecile a skos:Concept ;
    skos:prefLabel "Second decile"@en ;
    skos:definition "The 20th percentile, below which 20% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:93d51e9 .

stats:ThirdDecile a skos:Concept ;
    skos:prefLabel "Third decile"@en ;
    skos:definition "The 30th percentile, below which 30% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:85cba81 .

stats:FourthDecile a skos:Concept ;
    skos:prefLabel "Fourth decile"@en ;
    skos:definition "The 40th percentile, below which 40% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:2381913 .

stats:FifthDecile a skos:Concept ;
    skos:prefLabel "Fifth decile"@en ;
    skos:definition "The 50th percentile (median), below which 50% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:9eb1ff9 .

stats:SixthDecile a skos:Concept ;
    skos:prefLabel "Sixth decile"@en ;
    skos:definition "The 60th percentile, below which 60% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:380846d .

stats:SeventhDecile a skos:Concept ;
    skos:prefLabel "Seventh decile"@en ;
    skos:definition "The 70th percentile, below which 70% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:edf2add .

stats:EighthDecile a skos:Concept ;
    skos:prefLabel "Eighth decile"@en ;
    skos:definition "The 80th percentile, below which 80% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:03dbd00 .

stats:NinthDecile a skos:Concept ;
    skos:prefLabel "Ninth decile"@en ;
    skos:definition "The 90th percentile, below which 90% of data values fall."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3027971, ddi:9dea4c0 .

stats:OtherPercentile a skos:Concept ;
    skos:prefLabel "Other percentile"@en ;
    skos:definition "A percentile value not covered by quartiles, quintiles, or deciles."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q2913954, ddi:9947048 .

# Concepts - Robust Location/Scale

stats:Midrange a skos:Concept ;
    skos:prefLabel "Midrange"@en ;
    skos:definition "The average of the minimum and maximum values."@en ;
    skos:broader stats:RobustStatistics ;
    skos:inScheme stats: .

stats:Trimean a skos:Concept ;
    skos:prefLabel "Trimean"@en ;
    skos:definition "A weighted average of the median and two quartiles: (Q1 + 2×Q2 + Q3) / 4."@en ;
    skos:broader stats:RobustStatistics ;
    skos:inScheme stats: .

stats:BiweightMidvariance a skos:Concept ;
    skos:prefLabel "Biweight midvariance"@en ;
    skos:definition "A robust measure of scale based on the biweight (bisquare) function."@en ;
    skos:broader stats:RobustStatistics, stats:Dispersion ;
    skos:inScheme stats: .

stats:QnEstimator a skos:Concept ;
    skos:prefLabel "Qn scale estimator"@en ;
    skos:definition "A robust scale estimator based on the first quartile of pairwise absolute differences."@en ;
    skos:altLabel "Rousseeuw-Croux Qn"@en ;
    skos:broader stats:RobustStatistics, stats:Dispersion ;
    skos:inScheme stats: .

stats:SnEstimator a skos:Concept ;
    skos:prefLabel "Sn scale estimator"@en ;
    skos:definition "A robust scale estimator based on median of medians of pairwise absolute differences."@en ;
    skos:altLabel "Rousseeuw-Croux Sn"@en ;
    skos:broader stats:RobustStatistics, stats:Dispersion ;
    skos:inScheme stats: .

# Concepts - Association/Dependence

stats:Covariance a skos:Concept ;
    skos:prefLabel "Covariance"@en ;
    skos:definition "A measure of the joint variability between two variables."@en ;
    skos:broader stats:AssociationDependence ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q208273 .

stats:PearsonCorrelation a skos:Concept ;
    skos:prefLabel "Pearson correlation coefficient"@en ;
    skos:definition "A measure of linear correlation between two variables, ranging from -1 to +1."@en ;
    skos:broader stats:AssociationDependence ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q51993 .

stats:SpearmanCorrelation a skos:Concept ;
    skos:prefLabel "Spearman rank correlation"@en ;
    skos:definition "A non-parametric measure of rank correlation between two variables."@en ;
    skos:broader stats:AssociationDependence ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q79867 .

stats:KendallCorrelation a skos:Concept ;
    skos:prefLabel "Kendall rank correlation"@en ;
    skos:definition "A measure of ordinal association based on concordant and discordant pairs."@en ;
    skos:broader stats:AssociationDependence ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q1133707 .

stats:MaxAbsCorrelation a skos:Concept ;
    skos:prefLabel "Maximum absolute correlation (matrix summary)"@en ;
    skos:definition "The largest absolute correlation coefficient found in a correlation matrix."@en ;
    skos:broader stats:AssociationDependence ;
    skos:inScheme stats: .

# Concepts - Dispersion/Variability

stats:StandardErrorOfMean a skos:Concept ;
    skos:prefLabel "Standard error of the mean"@en ;
    skos:definition "The standard deviation of the sampling distribution of the mean."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q111972245, ddi:2c03973 .

stats:Range a skos:Concept ;
    skos:prefLabel "Range"@en ;
    skos:definition "The difference between the maximum and minimum values."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q1165839 .

stats:Variance a skos:Concept ;
    skos:prefLabel "Variance"@en ;
    skos:definition "The average of the squared deviations from the mean."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q175199 .

stats:StandardDeviation a skos:Concept ;
    skos:prefLabel "Standard deviation"@en ;
    skos:definition "The square root of the variance, measuring spread around the mean."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q159375 .

stats:AverageAbsoluteDeviation a skos:Concept ;
    skos:prefLabel "Average absolute deviation"@en ;
    skos:definition "The mean of the absolute deviations from a central point (typically the mean)."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q1067186, ddi:015ce0a .

stats:MedianAbsoluteDeviation a skos:Concept ;
    skos:prefLabel "Median absolute deviation"@en ;
    skos:definition "The median of the absolute deviations from the median, a robust measure of variability."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q3853702, ddi:5dc5781 .

stats:InterquartileRange a skos:Concept ;
    skos:prefLabel "Interquartile range"@en ;
    skos:definition "The difference between the third and first quartiles (Q3 - Q1)."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q1916617, ddi:a7f7339 .

stats:InterquintileRange a skos:Concept ;
    skos:prefLabel "Interquintile range"@en ;
    skos:definition "The difference between the fourth and first quintiles."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch ddi:b3ab24e .

stats:InterdecileRange a skos:Concept ;
    skos:prefLabel "Interdecile range"@en ;
    skos:definition "The difference between the ninth and first deciles (P90 - P10)."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:altLabel "P90-P10 range"@en ;
    skos:exactMatch wikidata:Q6046031, ddi:26ad7e5 .

stats:GiniCoefficient a skos:Concept ;
    skos:prefLabel "Gini Coefficient"@en ;
    skos:definition "A measure of statistical dispersion representing inequality in a distribution."@en ;
    skos:broader stats:Dispersion ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q162455 .

# Concepts - Size/Count

stats:ValidCases a skos:Concept ;
    skos:prefLabel "Valid cases"@en ;
    skos:definition "The count of observations with valid, non-missing values."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: ;
    skos:exactMatch ddi:c646dd8 .

stats:InvalidCases a skos:Concept ;
    skos:prefLabel "Invalid cases"@en ;
    skos:definition "The count of observations with invalid or out-of-range values."@en ;
    skos:broader stats:SizeCount, stats:MissingnessCompleteness ;
    skos:inScheme stats: ;
    skos:exactMatch ddi:6459c62 .

stats:NullCases a skos:Concept ;
    skos:prefLabel "Cases that have null/empty value"@en ;
    skos:definition "The count of observations with null, missing, or empty values."@en ;
    skos:broader stats:SizeCount, stats:MissingnessCompleteness ;
    skos:inScheme stats: .

stats:UniqueCases a skos:Concept ;
    skos:prefLabel "Unique Cases/Values"@en ;
    skos:definition "The count of distinct values in the dataset."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: .

stats:Sparsity a skos:Concept ;
    skos:prefLabel "Sparsity"@en ;
    skos:definition "The proportion of zero or null values in a dataset."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: .

stats:PositiveCount a skos:Concept ;
    skos:prefLabel "Count of positive values"@en ;
    skos:definition "The number of observations with positive values (> 0)."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: .

stats:ZeroCount a skos:Concept ;
    skos:prefLabel "Count of zero values"@en ;
    skos:definition "The number of observations with exactly zero value."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: .

stats:NegativeCount a skos:Concept ;
    skos:prefLabel "Count of negative values"@en ;
    skos:definition "The number of observations with negative values (< 0)."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: .

stats:SignificantDigits a skos:Concept ;
    skos:prefLabel "Number of digits needed to safely store the data"@en ;
    skos:definition "The precision or number of significant digits required to represent the values."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: .

stats:Frequency a skos:Concept ;
    skos:prefLabel "Frequency"@en ;
    skos:definition "The number of times a particular value or category appears."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q762856 .

stats:Other a skos:Concept ;
    skos:prefLabel "Other"@en ;
    skos:definition "A count or size measure not otherwise categorized."@en ;
    skos:broader stats:SizeCount ;
    skos:inScheme stats: ;
    skos:exactMatch ddi:6311ae1 .

# Concepts - Other/Derived Ratios

stats:ModeZScore a skos:Concept ;
    skos:prefLabel "Z-score of mode"@en ;
    skos:definition "The standardized distance of the mode from the mean in standard deviation units."@en ;
    skos:broader stats:DerivedRatios ;
    skos:inScheme stats: .

stats:StdDevRangeRatio a skos:Concept ;
    skos:prefLabel "Range to Standard Deviation Ratio"@en ;
    skos:definition "The ratio of the range to the standard deviation."@en ;
    skos:broader stats:DerivedRatios ;
    skos:inScheme stats: .

stats:CoefficientOfVariation a skos:Concept ;
    skos:prefLabel "Coefficient of variation"@en ;
    skos:definition "The ratio of the standard deviation to the mean, expressing variability relative to the mean."@en ;
