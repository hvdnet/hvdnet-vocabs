@prefix stats: <https://w3id.org/hvdnet/statistics/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wikidata: <https://wikidata.org/wiki/> .
@prefix ddi: <http://rdf-vocabulary.ddialliance.org/cv/SummaryStatisticType/2.1.2/> .

# Crosswalks: Concepts may include skos:exactMatch links to Wikidata
# using the wikidata: prefix above (https://wikidata.org/wiki/).
# Use skos:closeMatch where equivalence is weaker. Future revisions may
# adopt Wikidata entity IRIs (e.g., http://www.wikidata.org/entity/Q19033)
# for improved machine-resolution.

stats: a skos:ConceptScheme ;
    dct:title "Descriptive Statistics Vocabulary"@en ;
    dct:description "The descriptive statistics vocabulary provides a controlled set of concepts for describing statistical measures and summary statistics for FAIR data."@en ;
    dct:created "2025-12-20"^^xsd:date ;
    skos:hasTopConcept stats:CentralTendency, stats:Dispersion, stats:SizeCount, 
        stats:DerivedRatios, stats:ShapeDistribution, stats:TextMeasures, stats:Metadata, stats:FileLevel ;
    dct:license <https://creativecommons.org/publicdomain/zero/1.0/> .

# Categories

stats:CentralTendency a skos:Concept ;
    skos:prefLabel "Central Tendency/Location"@en ;
    skos:topConceptOf stats: ;
    skos:narrower 
        stats:ArithmeticMean, stats:GeometricMean, stats:HarmonicMean,
        stats:TrimmedMean, stats:WinsorizedMean, stats:Mode, stats:Median, 
        stats:Minimum, stats:Maximum, stats:Sum, 
        stats:FirstQuartile, stats:SecondQuartile, stats:ThirdQuartile, 
        stats:FirstQuintile, stats:SecondQuintile, stats:ThirdQuintile, stats:FourthQuintile, 
        stats:FirstDecile, stats:SecondDecile, stats:ThirdDecile, stats:FourthDecile, stats:FifthDecile, 
        stats:SixthDecile, stats:SeventhDecile, stats:EighthDecile, stats:NinthDecile, stats:OtherPercentile .

stats:Dispersion a skos:Concept ;
    skos:prefLabel "Dispersion/Variability"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:StandardErrorOfMean, stats:Range, stats:Variance, 
    stats:StandardDeviation, stats:AverageAbsoluteDeviation, stats:MedianAbsoluteDeviation, 
    stats:InterquartileRange, stats:InterquintileRange, stats:InterdecileRange, stats:GiniCoefficient .

stats:SizeCount a skos:Concept ;
    skos:prefLabel "Size/Count"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:ValidCases, stats:InvalidCases, stats:NullCases, stats:UniqueCases, 
    stats:Sparsity, stats:PositiveCount, stats:ZeroCount, stats:NegativeCount, 
    stats:SignificantDigits, stats:Frequency, stats:Other .

stats:DerivedRatios a skos:Concept ;
    skos:prefLabel "Other/Derived Ratios"@en ;
    skos:topConceptOf stats: ;  
    skos:narrower stats:ModeZScore, stats:StdDevRangeRatio, stats:CoefficientOfVariation, 
    stats:PercentageOfValidCases, stats:PercentageOfInvalidCases, stats:QuartileDispersionCoefficient .

stats:ShapeDistribution a skos:Concept ;
    skos:prefLabel "Shape/Distribution"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:Beta1, stats:Beta2, stats:BowleySkewnessCoefficient, stats:ShapiroWilk .

stats:TextMeasures a skos:Concept ;
    skos:prefLabel "Text/String Measures"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:MinLength, stats:MaxLength, stats:AverageLength, stats:LengthStdDev, 
    stats:MinWordCount, stats:MaxWordCount, stats:AverageWordCount, stats:TotalWordCount, 
    stats:UniqueWordCount, stats:LexicalDiversity, stats:Languages .

stats:Metadata a skos:Concept ;
    skos:prefLabel "Metadata"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:IsAscii .

stats:FileLevel a skos:Concept ;
    skos:prefLabel "File-level Statistics/Attributes"@en ;
    skos:topConceptOf stats: ;
    skos:narrower stats:FileRecordCount, stats:FileVariableCount, stats:FileSizeBytes, stats:FileChecksum, 
        stats:FileEncoding, stats:FileCompression, stats:FileSchemaVersion .

