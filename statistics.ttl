@prefix stats: <https://w3id.org/hvdnet/statistics/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wikidata: <https://wikidata.org/wiki/> .
@prefix ddi: <http://rdf-vocabulary.ddialliance.org/cv/SummaryStatisticType/2.1.2/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Crosswalks: Concepts may include skos:exactMatch links to Wikidata
# using the wikidata: prefix above (https://wikidata.org/wiki/).
# Use skos:closeMatch where equivalence is weaker. Future revisions may
# adopt Wikidata entity IRIs (e.g., http://www.wikidata.org/entity/Q19033)
# for improved machine-resolution.

stats: a skos:ConceptScheme ;
    dct:title "Descriptive Statistics Vocabulary [DRAFT]"@en ;
    dct:description "The descriptive statistics vocabulary provides a controlled set of concepts for describing statistical measures and summary statistics for FAIR data."@en ;
    dct:created "2025-12-20"^^xsd:date ;
    skos:hasTopConcept stats:AssociationDependence, stats:CategoricalProfiling, stats:CentralTendency, 
        stats:DerivedRatios, stats:Dispersion, stats:FileLevel, stats:InformationContent, 
        stats:Metadata, stats:MissingnessCompleteness, stats:Outliers, stats:RobustStatistics, 
        stats:ShapeDistribution, stats:SizeCount, stats:TextMeasures ;
    dct:license <https://creativecommons.org/publicdomain/zero/1.0/> .

# Property declarations (custom annotations)

stats:formula a rdf:Property ;
    rdfs:label "formula"@en ;
    rdfs:comment "Compact mathematical formula (e.g., LaTeX) describing how the statistic is computed."@en ;
    rdfs:domain skos:Concept ;
    rdfs:range xsd:string .

stats:computationMethod a rdf:Property ;
    rdfs:label "computation method"@en ;
    rdfs:comment "Brief textual steps describing how to compute the statistic, including conventions and exclusions."@en ;
    rdfs:domain skos:Concept ;
    rdfs:range xsd:string .

