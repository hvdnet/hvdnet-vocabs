stats:Sum a skos:Concept ;
    skos:prefLabel "Sum"@en ;
    skos:definition "The total obtained by adding all values together."@en ;
    skos:scopeNote "Add up all valid values; exclude missing or invalid entries."@en ;
    stats:formula "$\\sum_{i=1}^{n} x_i$" ;
    stats:computationMethod "Accumulate over non-missing values."@en ;
    skos:broader stats:CentralTendency ;
    skos:inScheme stats: ;
    skos:exactMatch wikidata:Q218005, ddi:a0ec870 .

